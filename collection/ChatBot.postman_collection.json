	"event": [
		{
			"listen": "test",
			"script": {
				"type": "text/javascript",
				"exec": [
					"// Collection-level test script - runs after each request",
					"// Logs request and response summary to console",
					"",
					"console.log('========================================');",
					"console.log('REQUEST SUMMARY');",
					"console.log('========================================');",
					"console.log('Request Name:', pm.info.requestName);",
					"console.log('Request ID:', pm.info.requestId);",
					"console.log('Method:', pm.request.method);",
					"console.log('URL:', pm.request.url.toString());",
					"",
					"if (pm.request.headers && pm.request.headers.count() > 0) {",
					"    console.log('Headers:');",
					"    pm.request.headers.each(function(header) {",
					"        if (!header.disabled) {",
					"            console.log('  ' + header.key + ': ' + header.value);",
					"        }",
					"    });",
					"}",
					"",
					"if (pm.request.body && pm.request.body.raw) {",
					"    console.log('Request Body:', pm.request.body.raw);",
					"}",
					"",
					"console.log('----------------------------------------');",
					"console.log('RESPONSE SUMMARY');",
					"console.log('----------------------------------------');",
					"console.log('Status Code:', pm.response.code);",
					"console.log('Status:', pm.response.status);",
					"console.log('Response Time:', pm.response.responseTime + 'ms');",
					"console.log('Response Size:', pm.response.responseSize + ' bytes');",
					"",
					"if (pm.response.headers && pm.response.headers.count() > 0) {",
					"    console.log('Response Headers:');",
					"    pm.response.headers.each(function(header) {",
					"        console.log('  ' + header.key + ': ' + header.value);",
					"    });",
					"}",
					"",
					"try {",
					"    const responseBody = pm.response.json();",
					"    console.log('Response Body (JSON):', JSON.stringify(responseBody, null, 2));",
					"} catch (e) {",
					"    const responseText = pm.response.text();",
					"    if (responseText) {",
					"        console.log('Response Body (Text):', responseText.substring(0, 500));",
					"    } else {",
					"        console.log('Response Body: (empty)');",
					"    }",
					"}",
					"",
					"console.log('========================================');",
					"console.log('Iteration:', pm.info.iteration + 1, 'of', pm.info.iterationCount);",
					"console.log('========================================');"
				]
			}
		}
